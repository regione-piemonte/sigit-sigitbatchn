<?xml version="1.0" encoding="UTF-8"?>
<deploy-descriptor 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:noNamespaceSchemaLocation="../../conf/deploy-descriptor.xsd">
	<rilascio
		ambiente="@rilascio.ambiente@"
		cliente="@rilascio.cliente@"
		istanza="@rilascio.istanza@"
		unita-installazione="sigitbatch"
		staging="false">
		<prodotto codice="SIGIT" versione="7.1.0">
			<descrizione>@prodotto.descrizione@</descrizione>
		</prodotto>
		<componente codice="SIGITBATCH">
			<descrizione>@componente.descrizione@</descrizione>
		</componente>
	</rilascio>
	<lista-servizi>
	</lista-servizi>
	<lista-binding>
	</lista-binding>
</deploy-descriptor>
